{
  "name": "@macalinao/tsconfig",
  "description": "Common TypeScript configuration.",
  "license": "Apache-2.0",
  "version": "1.0.1",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,cts,mts}": "eslint --fix --cache",
    "*.{md,json,yml,yaml,html}": "prettier --write"
  }
}
